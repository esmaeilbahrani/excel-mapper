{
  "name": "esmaeil/excelmapper",
  "description": "A PHP library that simplifies the process of mapping, parsing, and importing Excel data into your applications.",
  "type": "library",
  "keywords": [
    "php",
    "excel",
    "excel-mapper",
    "data-mapping",
    "spreadsheet"
  ],
  "autoload": {
    "psr-4": {
      "ExcelMapper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ExcelMapper\\Test\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0",
    "phpoffice/phpspreadsheet": "^1.20"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "authors": [
